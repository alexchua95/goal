<div class="welcome-carousel">
  <div class="welcome-carousel-slide-0">
    <div class="content">
      <h1>Pash</h1>
      <h3>Go out and learn</h3>
      <%= render 'landings/email_form', landing: Landing.new %>
    </div>
  </div>
  <div class="welcome-carousel-slide-1">
    <div class="content">
      <h2>What do you associate with?</h2>
      <div class="row text-center">
        <%= render 'landings/roles_form', landing: Landing.new %>
      </div>
    </div>
  </div>
  <div class="welcome-carousel-slide-2">
    <div class="content">
      <h2>What skills interest you?</h2>
      <div class="row text-center">
        <%= render 'landings/skills_form', landing: Landing.new %>
      </div>
    </div>
  </div>
</div>
